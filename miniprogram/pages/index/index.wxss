.index{
  background-color: #eae9e2;
}
.index .swiper-tab{
  width: 94%;
  margin-left: 3%;
  box-sizing: border-box;
  border-radius: 10rpx;
  height: 85rpx;
  padding-left: 15rpx;
  background-color: #e1dfd7;
  display: flex;
  padding-top: 20rpx;
  align-items: center;
}
.index .swiper-tab view{
  width: 50%;
  height: 95%;
  text-align: center;
  justify-content: center;
}
.index .swiper-tab .active{
  background-color: #eae8e0;
  border-radius: 8rpx;
  margin-top: -18rpx;
  margin-left: -5rpx;
  padding-top: 8rpx;
}
.index swiper-item view{
  color: #8d8479;
}
.index swiper{
  height: 1320rpx;
}
.slider{
  width: 1670rpx;
  display: flex;
    flex-wrap: wrap;
}
.slider .navigator{
  margin-top: 10rpx;
  border-radius: 15rpx;
  width: 320rpx;
  height: 390rpx;
  padding-left: 9.5rpx;
  border: 1px solid rgb(173, 171, 171);
  box-sizing: border-box;
  padding-top: 10rpx;
  margin-right: 15rpx;
}
.slider .navigator image{
  height: 85%;
  width: 295rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 5rpx;
  
}
.slider .navigator text{
  display: flex;
  justify-content: center;
  align-items: center;

}
.list{
  
}
.list .navigator{
  margin-top: 10rpx;
  margin-left: 10rpx;
  height: 220rpx;
  padding: 15rpx;
  box-sizing: border-box;
  border-bottom: 2px solid #fff;
  display: flex;
}
.list .navigator image{
  width: 300rpx;
  height: 190rpx;
}
.introduce,.name{
  width: 800rpx;
  margin-left: 10rpx;
}
.introduce text,.name text{
  font-size: 26rpx;
}
.show{
  background-color: #ededed;
  margin-top: 10rpx;
  padding-bottom: 10rpx;
  text-align: center;
}
.hidden{
  background-color: #ededed;
  margin-top: 10rpx;
  padding-bottom: 10rpx;
  text-align: center;
  display: none
}
.loading,.over{
  background-color: #ededed;
  margin-top: 10rpx;
  padding-bottom: 10rpx;
  text-align: center;
}

